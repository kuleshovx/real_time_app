CREATE VIEW sales_final AS
SELECT sales.date AS date, asu.id_asu AS id_asu, depot.id_depot AS id_depot, sales.version AS version, sales.sale AS sale, tank.id_tank AS id_tank, sales.plan_uuid AS plan_uuid 
FROM sales AS sales final
INNER JOIN asu AS asu final
ON sales.asu_uuid = asu.ext_id
INNER JOIN depot AS depot final
ON sales.depot_uuid = depot.ext_id
INNER JOIN tank AS tank final
ON sales.tank_uuid = tank.ext_id
WHERE not sales.is_delete 